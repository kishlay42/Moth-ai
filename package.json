{
  "name": "moth-ai",
  "version": "1.0.4",
  "description": "Local, LLM-agnostic code intelligence CLI",
  "main": "dist/index.js",
  "bin": {
    "moth": "./dist/index.js"
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "start": "node dist/index.js",
    "test": "node --import tsx --test tests/**/*.test.ts",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "cli",
    "llm",
    "ai",
    "code-assistant",
    "ollama",
    "openai"
  ],
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "docs"
  ],
  "author": "Moth Contributor",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kishlay42/Moth-ai.git"
  },
  "type": "module",
  "devDependencies": {
    "@types/diff": "^7.0.2",
    "@types/js-yaml": "^4.0.9",
    "@types/keytar": "^4.4.0",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.7.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "commander": "^14.0.2",
    "diff": "^8.0.2",
    "ignore": "^7.0.5",
    "ink": "^6.6.0",
    "ink-big-text": "^2.0.0",
    "ink-gradient": "^3.0.0",
    "ink-select-input": "^6.2.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "js-yaml": "^4.1.1",
    "keytar": "^7.9.0",
    "react": "^19.2.3"
  }
}
